@using BasicRestaurant.Models;

<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Basic Restaurant</title>
</head>
<body>

  <div class="container">
    @if(Model.Count == 0)
    {
      <p>There are currently no businesses.</p>
    }
    @foreach (Business business in Model)
    {
      <p>@business.Name</p>
    }

    <hr>

    @foreach (BoardMember boardmember in @ViewBag.Boards)
    {
      <p>@boardmember.Name</p>
    }
  </div>
</body>
</html>